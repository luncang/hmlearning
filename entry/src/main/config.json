{
  "app": {
    "bundleName": "com.example.hmlearning",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.hmlearning",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.example.hmlearning.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "permissions": [
          "com.example.hmlearning.ui.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "com.example.hmlearning.ui.MyDataAbility",
        "icon": "$media:icon",
        "visible": true,
        "description": "$string:mydataability_description",
        "type": "data",
        "uri": "dataability://com.example.hmlearning.ui.MyDataAbility"
      },
      {
        "name": "com.example.hmlearning.ui.MyServiceAbility",
        "icon": "$media:icon",
        "description": "$string:myserviceability_description",
        "type": "service",
        "visible": true,
        "backgroundModes": [
          "dataTransfer",
          "audioPlayback",
          "audioRecording",
          "pictureInPicture",
          "voip",
          "location",
          "bluetoothInteraction",
          "wifiInteraction"
        ]
      },
      {
        "orientation": "portrait",
        "name": "com.example.hmlearning.ui.FirstAbility",
        "icon": "$media:icon",
        "description": "$string:firstability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "skills": [
          {
            "actions": [
              "action.firstability.first",
              "action.firstability.second"
            ]
          }
        ]
      },
      {
        "orientation": "unspecified",
        "name": "com.example.hmlearning.ui.SecondAbility",
        "icon": "$media:icon",
        "description": "$string:firstability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.hmlearning.ui.ContinuationAbility",
        "icon": "$media:icon",
        "description": "$string:continuationability_description",
        "label": "$string:entry_ContinuationAbility",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}